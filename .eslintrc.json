{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-undef": "off",
        "func-names": "off",
        "new-cap": "off",
        "no-shadow": "off",
        "no-unused-vars": "off",
        "no-restricted-syntax": "off",
        "linebreak-style": "off",
        "global-require": 0,
        "no-plusplus": "off"
    }
}
